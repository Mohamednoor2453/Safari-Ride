{
  "expo": {
    "name": "Safari Ride",
    "slug": "safari_ride",
    "scheme": "safariride",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mohamednoor.safariride",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Safari Ride needs your location to find nearby drivers and rides.",
        "NSLocationAlwaysUsageDescription": "Safari Ride uses your location to show nearby drivers and provide accurate rides."
      },
      "config": {
        "googleMapsApiKey": "YOUR_IOS_GOOGLE_MAPS_API_KEY"
      }
    },
    "android": {
      "package": "com.mohamednoor.safariride",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "INTERNET"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_ANDROID_GOOGLE_MAPS_API_KEY"
        }
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-location",
      "expo-secure-store"
    ],
    "extra": {
      "googleApiKey": "YOUR_GOOGLE_API_KEY",
      "router": {},
      "eas": {
        "projectId": "d2bbd348-653b-4c97-8139-a3bcef8e4b92"
      }
    },
    "owner": "mohamadeit"
  }
}
